; labels are written as a name without whitespace
; and a colon at the end

; jumping to labels is not yet supported by the
; assembler

main:
  ; this is a comment

   72 out          ; "H"
  101 out          ; "e"
  108 dup out out  ; "ll"
  111 out          ; "o"
   33 out          ; "!"

  ; newline
  10 13 out out

  42 outnum     ; print number
  10 13 out out ; ... and CRLF

  ; stop program
  halt
